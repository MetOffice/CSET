{% if DETERMINISTIC_PLOT_CAPE_RATIO|default(False) %}
{% for model in models %}
[runtime]
    [[plot_cape_ratio_m{{model["number"]}}]]
    inherit = PROCESS
        [[[environment]]]
        CSET_RECIPE_NAME = "CAPE_ratio_plot.yaml"
        CSET_ADDOPTS = """
            --MODEL_NAME='{{model["name"]}}'
        """
        MODEL_NUMBER = {{model["number"]}}
{% endfor %}
{% endif %}
