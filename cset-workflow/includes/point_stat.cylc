{% if METPLUS_POINT_STAT %}
[scheduling]
    [[graph]]
    {{CSET_CYCLE_PERIOD}} = """
    metplus_ascii2nc => metplus_point_stat => housekeeping_full
    """

[runtime]
    [[metplus_ascii2nc]]
    # Runs METplus wrappers for ASCII to nc ingestion of obs.
        inherit = METPLUS

    [[metplus_point_stat]]
    # Runs METplus wrappers for point stat calculations.
        inherit = METPLUS
{% endif %}
